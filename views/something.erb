<head>


	<!-- functions here -->
	<script>
		$(function(){
			$(document).on("click", "a.alert", function(e) {
				bootbox.confirm({
				    message: "This is a confirm with custom button text and color!",
				    buttons: {
				        confirm: {
				            label: 'Yes',
				            className: 'btn-success'
				        },
				        cancel: {
				            label: 'No',
				            className: 'btn-danger'
				        }
				    },
				    callback: function (result) {
				        console.log('This was logged in the callback: ' + result);
				    }
				});
			});
			mouseEnterLeaveActions("#text1");
		});
	</script>

</head>

<body>
	

	<p>Content here. <a id="text1" class="alert" href=#>Alert!</a></p>


</body>
